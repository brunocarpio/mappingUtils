{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "18"
                },
                "useBuiltIns": "usage",
                "corejs": 3,
                "modules": false
            }
        ]
    ],
    "env": {
        "cjs": {
            "plugins": ["@babel/plugin-transform-modules-commonjs"]
        }
    }
}
